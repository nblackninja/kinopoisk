{
  "private": true,
  "name": "kinopoisk",
  "version": "1.17.0",
  "license": "MIT",
  "description": "Internet service about movies. Movie search, movie news and more.",
  "author": "Nikita Yudin <nikitayudin782@gmail.com>",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "homepage": "https://github.com/nblackninja/kinopoisk#readme",
  "bugs": {
    "url": "https://github.com/nblackninja/kinopoisk/issues",
    "email": "nikitayudin782@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nblackninja/kinopoisk.git"
  },
  "keywords": [
    "js",
    "javascript",
    "typescript",
    "react",
    "nextjs",
    "kinopoisk"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "yarn.lock",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.26.1",
    "embla-carousel-react": "^6.2.0",
    "msw": "^0.39.2",
    "next": "12.1.5",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-player": "^2.10.0",
    "react-redux": "^8.0.0",
    "styled-components": "^5.3.5",
    "use-debounce": "^7.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "17.0.24",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.1",
    "@types/styled-components": "^5.1.25",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-config-next": "12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "prettier": "^2.6.2",
    "semantic-release": "^19.0.2",
    "typescript": "4.6.3"
  }
}
